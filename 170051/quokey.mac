	.PAGE
	.SBTTL	DEFINE AND ORDER THE .PSECTS

		.PSECTS	QUOKEY,RW,D,GBL,REL,OVR


	.PAGE
	.SBTTL	INCLUDE CUI MACROS

		.MCALL	$CUI
		$CUI	TABLE				; DEFINE CUI TABLES


	.PAGE
	.SBTTL	DEFINE QUOKEY MACROS

	; ASSIGN EVEN VALUES TO COMMAND/QUALIFIER KEYS

		.MACRO	KEYDEF	KEY,KEYNAM
		DEFKEY	KEY,<KEYNAM>
		TMPORG	QUOKEY
		.WORD	KEY
		UNORG
		.ENDM	KEYDEF

	; ASSIGN ODD VALUES TO PARAMETER TAGS

		.MACRO	TAGDEF	TAG
		.PSECT	DUMMY,ABS
		.IF	NDF	TAGORG
		TAGORG:
		.ENDC
		.IF	EQ	.-TAGORG
		.BLKB	1
		.ENDC
		TAG::	.BLKB	2
		UNORG
		TMPORG	QUOKEY
		.WORD	TAG
		UNORG
		.ENDM TAGDEF


	.PAGE
	.SBTTL QUOLST COMMAND KEYWORDS

		KEYDEF	SHO.K,<"SH-OW QUO-TA">		; SHOW COMMAND

	; DEFINE KEYWORDS

		KEYDEF	OUT.Q,"OU-TPUT"			; /[NO]OUTPUT	   ;01

		.PAGE
		.SBTTL	QUOLST PARAMETER TAGS

		TAGDEF	PPN.T				; KB PARAMETER TAG

		.END
